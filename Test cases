SELECT 
    c.customer_id,
    c.first_name, 
    c.last_name, 
    a.house_number, 
    a.street, 
    a.postcode, 
    a.id, 
    d.spouse, 
    d.pets
FROM 
    customers c 
JOIN 
    address a ON c.house_number = a.house_number 
LEFT JOIN 
    dependents d ON c.customer_id = d.customer_id
GROUP BY 
    c.customer_id, c.first_name, c.last_name;
